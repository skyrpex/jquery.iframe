{
  "extends": [
    "standard",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "settings": {
    "import/ignore": [
      "node_modules",
      "\\.html$"
    ],
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.babel.js"
      }
    }
  },
  "rules": {
    "import/no-unresolved": 1
  }
}
